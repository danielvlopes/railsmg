- semantic_form_for @meeting, :html => { :multipart => true } do |f|
  - f.inputs do
    = f.input :name
    = f.input :user
    = f.input :description, :input_html => { :size => '40x5' }
    = f.input :video
    = f.input :code
    = f.input :slide_preview
    = f.input :slides
    = f.input :start_on, :start_year => 2010

  - f.buttons do
    = f.commit_button 'Cadastrar'

:javascript
  document.getElementById('name').focus();
